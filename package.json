  "scripts": {
    "test": "node scripts/test.js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "start_dev": "run-script-os",
    "start_prod": "run-script-os",
    "start_local": "run-script-os",
    "start_wehagov": "run-script-os",
    "build_dev": "run-script-os",
    "build_prod": "run-script-os",
    "build_wehagov": "run-script-os",
    "start_dev:windows": "set REACT_APP_DEPLOY_TYPE=development&& node scripts/start.js",
    "start_prod:windows": "set REACT_APP_DEPLOY_TYPE=production&& node scripts/start.js",
    "start_local:windows": "set REACT_APP_DEPLOY_TYPE=local&& node scripts/start.js",
    "start_wehagov:windows": "set REACT_APP_DEPLOY_TYPE=wehagov&& node scripts/start.js",
    "build_dev:windows": "set REACT_APP_DEPLOY_TYPE=development&&set NODE_ENV=development&& node --max-old-space-size=3072 scripts/build.js",
    "build_prod:windows": "set REACT_APP_DEPLOY_TYPE=production&&set NODE_ENV=production&& node --max-old-space-size=3072 scripts/build.js",
    "build_wehagov:windows": "set REACT_APP_DEPLOY_TYPE=wehagov&&set NODE_ENV=wehagov&& node --max-old-space-size=3072 scripts/build.js",
    "start_dev:linux:darwin": "REACT_APP_DEPLOY_TYPE=development node scripts/start.js",
    "start_prod:linux:darwin": "REACT_APP_DEPLOY_TYPE=production node scripts/start.js",
    "start_local:linux:darwin": "REACT_APP_DEPLOY_TYPE=local node scripts/start.js",
    "start_wehagov:linux:darwin": "REACT_APP_DEPLOY_TYPE=wehagov node scripts/start.js",
    "build_dev:linux:darwin": "REACT_APP_DEPLOY_TYPE=development NODE_ENV=development node --max-old-space-size=3072 scripts/build.js",
    "build_prod:linux:darwin": "REACT_APP_DEPLOY_TYPE=production NODE_ENV=production node --max-old-space-size=3072 scripts/build.js",
    "build_wehagov:linux:darwin": "REACT_APP_DEPLOY_TYPE=wehagov NODE_ENV=wehagov node --max-old-space-size=3072 scripts/build.js"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "/Users/byun/wehago-ui-starter/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "react-error-overlay": "6.0.9"
  }
}
